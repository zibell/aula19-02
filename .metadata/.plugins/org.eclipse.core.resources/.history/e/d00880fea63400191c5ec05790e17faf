package entidade;

public class Espirito {

		private Integer id;
		private String nome;
		
		public Espirito () {
			
		}

		public Espirito(Integer id, String nome) {
			super();
			this.id = id;
			this.nome = nome;
		}

		public Integer getId() {
			return id;
		}

		public void setId(Integer id) {
			this.id = id;
		}

		public String getNome() {
			return nome;
		}

		public void setNome(String nome) {
			this.nome = nome;
		}
		
		
		
		
		
		
}
